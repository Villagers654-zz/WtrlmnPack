  "mob_effect_grid_panel": {
    "type": "panel",
    "size": [ "100%", "100%c" ],
    "controls": [
      {
        "mob_effect_grid@mob_effect.mob_effect_grid": {
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "size": [ "100%", "default" ],
          "offset": [ 0, 0 ],
          "grid_item_template": "mob_effect_dx.mob_effect_grid_item"
        }
      }
    ]
  },

  "main_mob_effect_scroll_panel": {
    "type": "panel",
    "size": [ 180, 160 ],
    "controls": [
      { "bg@chat_dx.chat_bg": { } },
      {
        "scrolling_panel@common_scroll_dx.scrolling_panel": {
          "layer": 10,
          "size": [ "100%", "100%" ],
          "$scroll_size": [ 4, "100%" ],
          "$scrolling_pane_size": [ "100%", "100%" ],
          "$scroll_view_port_size": [ "100%", "100%" ],
          "$scroll_view_port_size_touch": [ "100%", "100%" ],
          "$scrolling_pane_offset": [ 0, 0 ],
          "$scroll_box_image_control": "chat_dx.scroll_box",
          "$scrolling_content": "mob_effect_dx.mob_effect_grid_panel",
          "$jump_to_bottom_on_update": true,
          "$always_handle_scrolling": true
        }
      }
    ]
  },

  "main_screen": {
    "type": "panel",
    "size": [ "100%", "100%" ],
    "controls": [
      { "top_buttons@chat_dx.top_buttons": { } },
      { "main_mob_effect_scroll_panel@mob_effect_dx.main_mob_effect_scroll_panel": { } },
      { "screen_background@pause_dx.screen_background": { "layer": -10, "ignored": "$is_holographic" } }
    ]
