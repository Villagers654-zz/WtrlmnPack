  "highlight_slot_red@java_gui_common.highlight_slot": {
    "layer": 0,
    "texture": "textures/uidx/highlight/highlight_slot_red"
  },

  "highlight_slot_output@java_gui_common.highlight_slot": {
    "texture": "textures/uidx/highlight/highlight_slot_output",
    "size": [ 24, 24 ]
  },

  "highlight_slot_output_red@java_gui_common.highlight_slot": {
    "layer": 0,
    "texture": "textures/uidx/highlight/highlight_slot_output_red",
    "size": [ 24, 24 ]
