{
  "namespace": "java_gui_common",

  /*//// UV and UV size image panel ////*/

  "crisxolt_image_panel": {
    "type": "panel",
    "size": "$uv_size",
    "$base_anchor": "top_left",
    "$base_size": [ 256, 256 ],
    "clips_children": true,
    "$uv": [ 0, 0 ],
    "$uv_size": [ "100%", "100%" ],
    "controls": [
      {
        "main_image": {
          "type": "image",
          "anchor_from": "$base_anchor",
          "anchor_to": "$base_anchor",
          "size": "$base_size",
          "offset": "$uv",
          "texture": "$texture"
        }
      }
    ]
  },

  /*//// Screen BG ////*/

  "screen_background": {
    "type": "image",
    "size": [ "100%", "100%" ],
    "texture": "textures/uidx/screen_background"
  },

  /*//// Panorama Overlay ////*/

  "panorama_overlay_image": {
    "type": "image",
    "size": [ "100%", "100%" ],
    "alpha": 0.0,
    "texture": "assets/minecraft/textures/gui/title/background/panorama_overlay"
  },

  /*//// Edition ////*/

  "edition_image": {
    "type": "image",
    "size": [ 128, 16 ],
    "texture": "assets/minecraft/textures/gui/title/edition"
  },

  /*//// Cell Image and Overlays ////*/

  "cell_image": {
    "type": "panel",
    "size": [ "100%", "100%" ]
  },

  "cell_image_output": {
    "type": "panel",
    "size": [ 26, 26 ]
  },

  /*//// Highlight ////*/

